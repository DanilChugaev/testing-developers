{
  "name": "testing-developers",
  "version": "0.1.0",
  "description": "Project for testing developers",
  "main": "src/index.js",
  "scripts": {
    "start": "pm2 restart launchServer.sh",
    "server": "NODE_ENV=development nodemon src/index.js --exec babel-node",
    "debug": "NODE_ENV=development nodemon --inspect src/index.js  --exec babel-node",
    "client-install": "npm install --prefix client",
    "client": "npm run dev --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "kill": "killall -9 node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanilChugaev/testing-developers.git"
  },
  "keywords": [
    "node.js",
    "vue.js",
    "graphql",
    "spa",
    "testing",
    "developers"
  ],
  "author": "Danil Chugaev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DanilChugaev/testing-developers/issues"
  },
  "homepage": "https://github.com/DanilChugaev/testing-developers#readme",
  "dependencies": {
    "apollo-boost": "^0.1.16",
    "apollo-server": "^2.25.4",
    "apollo-server-express": "^2.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.4",
    "express-graphql": "^0.6.12",
    "graphql": "^14.0.2",
    "graphql-relay": "^0.5.5",
    "graphql-sequelize": "^9.0.4",
    "graphql-tools": "^4.0.2",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.5.0",
    "pg-hstore": "^2.3.2",
    "postgraphql": "^3.5.6",
    "sequelize": "^4.41.0",
    "vue-apollo": "^3.0.0-beta.25"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.4"
  },
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ]
  }
}
