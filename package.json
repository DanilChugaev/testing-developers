{
  "name": "testing-developers",
  "version": "0.1.0",
  "description": "Project for testing developers",
  "main": "index.mjs",
  "scripts": {
    "server": "nodemon --experimental-modules index.mjs",
    "debug": "nodemon --inspect --experimental-modules index.mjs",
    "client-install": "npm install --prefix client",
    "client": "npm run dev --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "kill": "killall -9 node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanilChugaev/testing-developers.git"
  },
  "keywords": [
    "node.js",
    "vue.js",
    "graphql",
    "spa",
    "testing",
    "developers"
  ],
  "author": "Danil Chugaev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DanilChugaev/testing-developers/issues"
  },
  "homepage": "https://github.com/DanilChugaev/testing-developers#readme",
  "dependencies": {
    "apollo-server": "^2.1.0",
    "apollo-server-express": "^2.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.4",
    "express-graphql": "^0.6.12",
    "graphql": "^14.0.2",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.4"
  },
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ]
  }
}
